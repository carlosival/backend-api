{% extends 'base.html.twig' %}

{% block body %}
    <h1>Dietaitems list</h1>

    <table>
        <thead>
            <tr>
                <th>Id</th>
                <th>Clasificacion</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for dietaItem in dietaItems %}
            <tr>
                <td><a href="{{ path('dietaitem_show', { 'id': dietaItem.id }) }}">{{ dietaItem.id }}</a></td>
                <td>{{ dietaItem.clasificacion }}</td>
                <td>
                    <ul>
                        <li>
                            <a href="{{ path('dietaitem_show', { 'id': dietaItem.id }) }}">show</a>
                        </li>
                        <li>
                            <a href="{{ path('dietaitem_edit', { 'id': dietaItem.id }) }}">edit</a>
                        </li>
                    </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('dietaitem_new') }}">Create a new dietaItem</a>
        </li>
    </ul>
{% endblock %}
